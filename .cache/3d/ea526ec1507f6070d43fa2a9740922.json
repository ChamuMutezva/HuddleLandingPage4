es/bg-hero-mobile.svg\");\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    background-color: utils.$bg-lime;\r\n\r\n    .logoSection {\r\n        padding: 1rem 1.5rem;\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n    }\r\n\r\n    .logo {\r\n        width: 65%;\r\n        max-width: 200px;\r\n    }\r\n\r\n    .tryIt,\r\n    .logo__link {\r\n        flex: 1 1 50%;\r\n    }\r\n\r\n    .tryIt {\r\n        display: flex;\r\n        justify-content: flex-end;\r\n        align-items: center;\r\n    }\r\n\r\n    // hero section\r\n    .hero {\r\n        padding: 3.5rem 2.25rem 2.5rem;\r\n        margin: auto;\r\n        text-align: center;\r\n    }\r\n    .hero__summary {\r\n        margin: 2rem auto;\r\n    }\r\n\r\n    .img__container {\r\n        margin-top: 3rem;\r\n    }\r\n    \r\n}","scss\\layout\\_main.scss":"@use '../utils';\r\n.main {\r\n\tpadding: 3rem 1.25rem;\r\n    .primary {\r\n        box-shadow: -1px 3px 6px 5px #979797;\r\n        border-radius: 20px;\r\n        padding: 2rem;\r\n        margin-bottom: 2rem;\r\n    }\r\n    .primary__sub {\r\n        padding-top: 3rem;\r\n        text-align: center;\r\n    }\r\n    .primary__summary {\r\n        font-size: 0.875rem;\r\n        color: utils.$tx-mid-grey;\r\n    }\r\n    .primary__title {\r\n        font-size: 1.25rem;\r\n        margin-bottom: 1rem;\r\n    }\r\n   \r\n}\r\n","scss\\layout\\_aside.scss":"@use '../utils';\r\n\r\n.get__ready {\r\n\r\n    position: relative;\r\n    top: 2rem;\r\n    padding: 3rem 1.25rem;\r\n\r\n    .aside__container {\r\n        box-shadow: 2px 2px 2px 2px #979797;\r\n        border-radius: 20px;\r\n        padding: 2rem 0;\r\n        z-index: 2;\r\n        position: relative;\r\n        background: white;\r\n    }\r\n\r\n    .secondary__title {\r\n        font-size: 1.125rem;\r\n        text-align: center;\r\n        color: utils.$tx-green-black;\r\n        margin-bottom: 2rem;\r\n    }\r\n\r\n}","scss\\layout\\_footer.scss":"@use '../utils';\r\n\r\n.footer {\r\n    background-color: #00252e;\r\n    color: utils.$tx-white;\r\n    padding: 8rem 1rem 4rem;\r\n    margin-top: -6rem;\r\n\r\n    .logoFooter {\r\n        transform: scale(.6);\r\n        margin-left: -48px;\r\n        padding-left: 0;\r\n        display: block;\r\n    }\r\n\r\n    .logoFooter path {\r\n        fill: utils.$tx-white;\r\n    }\r\n\r\n    .contact {\r\n        display: flex;\r\n        justify-content: flex-start;\r\n        align-items: baseline;\r\n    }\r\n    .fa {\r\n        margin-right: 20px;\r\n    }\r\n}","scss\\component\\_button.scss":"@use '../utils';\r\n.btn__try {\r\n    background-color: utils.$bg-white;\r\n    font-size: 0.625rem;\r\n    padding: .5rem 1.5rem;\r\n    border-radius: 12px;\r\n    font-weight: 700;\r\n    color: utils.$tx-green-black;\r\n    text-decoration: none;\r\n    text-align: center;\r\n}\r\n.get__started {\r\n    background-color: utils.$bg-btn-pink;\r\n\tpadding: .8rem 2rem;\r\n\twidth: 80%;\r\n\tdisplay: block;\r\n\tmargin: auto;\r\n\tmax-width: 240px;\r\n    font-size: 0.75rem;\r\n    color: utils.$tx-white;\r\n    border-radius: 20px;\r\n}"},"lineCount":null}},"error":null,"hash":"82e6108b96e9ec67773b3d07f5a2d47d","cacheData":{"env":{}}}